{
  "name": "shippable",
  "version": "4.0.1",
  "dependencies": {
    "async": "2.0.0",
    "underscore": "1.8.3",
    "fs-extra": "0.30.0"
  },
  "jshintConfig": {
    "node": true,
    "jquery": true,
    "undef": true,
    "noarg": true,
    "white": true,
    "esnext": true,
    "strict": true,
    "newcap": true,
    "nonbsp": true,
    "eqeqeq": true,
    "unused": true,
    "maxlen": 80,
    "indent": 2,
    "bitwise": true,
    "noempty": true,
    "latedef": "nofunc",
    "maxdepth": 5,
    "quotmark": "single",
    "trailing": true,
    "maxparams": 18,
    "globalstrict": true,
    "maxcomplexity": 20,
    "maxstatements": 50,
    "predef": [
      "logger",
      "process",
      "config",
      "AMQP",
      "server",
      "util",
      "async",
      "ActErr",
      "msName",
      "_"
    ]
  }
}
